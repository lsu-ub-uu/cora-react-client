{
	"name": "diva-react-client",
	"version": "1.0.0",
	"description": "A react client for DiVA.",
	"dependencies": {
		"@types/react-router": "^5.1.18",
		"axios": "^0.27.2",
		"diva-cora-ts-api-wrapper": "file:../diva-cora-ts-api-wrapper",
		"grommet": "^2.25.1",
		"grommet-icons": "^4.7.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router": "^6.3.0",
		"react-router-dom": "^6.3.0",
		"styled-components": "^5.3.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.10",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-react": "^7.18.6",
		"@babel/preset-typescript": "^7.18.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^12.1.5",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^28.1.6",
		"@types/node": "^18.7.3",
		"@types/react": "^17.0.47",
		"@types/react-dom": "^17.0.17",
		"@types/styled-components": "^5.1.26",
		"@types/testing-library__jest-dom": "^5.14.5",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"buffer": "^6.0.3",
		"diva-resource-fetcher": "file:../diva-resource-fetcher",
		"eslint": "^8.22.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.4.1",
		"eslint-plugin-html": "^7.1.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.30.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"jest": "^28.1.3",
		"jest-environment-jsdom": "^28.1.3",
		"parcel": "^2.7.0",
		"parcel-reporter-static-files-copy": "^1.5.0",
		"prettier": "^2.7.1",
		"process": "^0.11.10",
		"ts-jest": "^28.0.8",
		"typescript": "^4.7.4",
		"whatwg-fetch": "^3.6.2"
	},
	"scripts": {
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build": "npm run build:types && npm run build:js",
		"build:prod": "npm run build:types && parcel build ./src/index.html --dist-dir ./dist --public-url $BASENAME",
		"build:local": "npm run build:types && parcel build ./src/index.html --dist-dir ./dist --public-url /",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
		"start": "parcel -p 3000 src/index.html",
		"eslint": "npx eslint \"src/**/*\" --quiet --fix/;exit 0",
		"test": "jest --watch",
		"test:file": "jest --watch --testPathPattern",
		"coverage": "npm run coverage:dev /; npm run coverage:show",
		"coverage:dev": "jest --coverage /;exit 0",
		"coverage:deploy": "jest --coverage",
		"coverage:show": "firefox ~/workspace/diva-react-client/diva-react-client/coverage/lcov-report/index.html",
		"fetch-diva-resources": "npx divaResourceFetcher ./lists/"
	},
	"author": "Library Systems Unit - Uppsala University Library",
	"license": "GPL-3.0"
}
