{
	"name": "diva-react-client",
	"version": "1.0.0",
	"description": "A react client for DiVA.",
	"main": "index.js",
	"dependencies": {
		"@types/react-router": "^5.1.17",
		"axios": "^0.24.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-router": "^6.0.2",
		"react-router-dom": "^6.0.2",
		"styled-components": "^5.3.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.16.0",
		"@babel/core": "^7.16.0",
		"@babel/plugin-proposal-class-properties": "^7.16.0",
		"@babel/preset-env": "^7.16.4",
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/react-hooks": "^7.0.2",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.0.3",
		"@types/node": "^16.11.12",
		"@types/react": "^17.0.37",
		"@types/react-dom": "^17.0.11",
		"@types/styled-components": "^5.1.17",
		"@types/testing-library__jest-dom": "^5.14.2",
		"@types/testing-library__react": "^10.2.0",
		"@typescript-eslint/eslint-plugin": "^5.6.0",
		"@typescript-eslint/parser": "^5.6.0",
		"eslint": "^8.4.1",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-config-prettier": "^8.3.0",
		"eslint-import-resolver-typescript": "^2.5.0",
		"eslint-plugin-html": "^6.2.0",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-hooks": "^4.3.0",
		"jest": "^27.4.4",
		"parcel": "^2.0.1",
		"prettier": "^2.5.1",
		"ts-jest": "^27.1.2",
		"typescript": "^4.5.3",
		"whatwg-fetch": "^3.6.2"
	},
	"scripts": {
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"build": "npm run build:types && npm run build:js",
		"build:types": "tsc --emitDeclarationOnly",
		"build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
		"start": "parcel -p 3000 src/index.html",
		"eslint": "npx eslint \"src/**/*\" \"__tests__/**/*\" --quiet --fix/;exit 0",
		"test": "jest --watch",
		"test:file": "jest --watch --testPathPattern",
		"coverage": "npm run coverage:dev /; npm run coverage:show",
		"coverage:dev": "jest --coverage /;exit 0",
		"coverage:deploy": "jest --coverage",
		"coverage:show": "firefox ~/workspace/diva-react-client/coverage/lcov-report/index.html"
	},
	"author": "Library Systems Unit - Uppsala University Library",
	"license": "GPL-3.0"
}
